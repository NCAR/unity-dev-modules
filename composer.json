{
    "name": "ncar/unity-dev-modules",
    "description": "Composer package containing Drupal development modules",
    "type": "metapackage",
    "license": "proprietary",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "10.4.7",
        "phpmd/phpmd": "^2.9",
        "phpspec/prophecy-phpunit": "^2",
        "squizlabs/php_codesniffer": "^3.5",
        "drupal/entity_browser": "^2.0@dev",
        "phpro/grumphp": "^2.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "phpro/grumphp": true
        }
    }
}
